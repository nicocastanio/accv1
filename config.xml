<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "ar.com.dev.accuda"
        version     = "1.0.0" >

    <!-- versionCode is optional and Android only -->

    <name>PhoneGap Acuda</name>

    <description>
        An example for Acuda App using phonegap build . 
    </description>

    <author href="https://www.gmail.com" email="nicocastanio@gmail.com">
        Nicolás Castaño 
    </author>

    <content src="index.html" />
        
    <gap:platform name="ios" />
    <gap:platform name="android" />
    <gap:platform name="winphone" />
        
    <gap:plugin name="de.appplant.cordova.plugin.email-composer" version="0.8.1" />
    <gap:plugin name="tr.bel.mamak.sms_plagin" version="0.1.0" />
    
    <icon src="icon.png" width="60" height="60" />

</widget>
